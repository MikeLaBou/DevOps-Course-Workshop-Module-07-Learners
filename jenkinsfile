pipeline {
    agent any
    
        docker {
            image 'myjenkins-blueocean:1.1'
        }
    }
    stages {
        stage('Build') {
            steps {
                echo 'Testing. docker.'
            }
        }
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}